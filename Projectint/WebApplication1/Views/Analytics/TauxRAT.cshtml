    <script>
var values = @Html.Raw(Json.Encode(ViewBag.NbrState));
    var labels = @Html.Raw(Json.Encode(ViewBag.DateRAT));
                var barData = {
                labels : labels,
                datasets : [
                    {
                        fillColor : "#81F79F",
                        strokeColor : "#2EFE9A",
                        data : values
                    }
                ]
            }
			var income = document.getElementById("income").getContext("2d");
			   new Chart(income).Bar(barData);

    </script>
    <script type="text/javascript">
        var raht = @Html.Raw(Json.Encode(ViewBag.raht));
        var raft = @Html.Raw(Json.Encode(ViewBag.raft));
        google.charts.load("current", {packages:["corechart"]});
        google.charts.setOnLoadCallback(drawChart);
        function drawChart() {
            var data = google.visualization.arrayToDataTable([
              ['Language', 'Speakers (in millions)'],
              ['Homme',  raht],
              ['Femme', raft]

            ]);

            var options = {
                legend: 'none',
                pieSliceText: 'label'
  

            };

            var chart = new google.visualization.PieChart(document.getElementById('piechart'));
            chart.draw(data, options);
        }
    </script>
    <script type="text/javascript">
        var tat218 = @Html.Raw(Json.Encode(ViewBag.tat218));
        var tat1935 = @Html.Raw(Json.Encode(ViewBag.tat1935));
        var tat3664 = @Html.Raw(Json.Encode(ViewBag.tat3664));
        var tat65 = @Html.Raw(Json.Encode(ViewBag.tat65));

      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Task', 'Hours per Day'],
          ['2-18 Ans',     tat218],
          ['19-35 Ans',      tat1935],
          ['36-64 Ans',  tat3664],
          ['Plus que 65 Ans', tat65]
        ]);

        var options = {
        
          pieHole: 0.4
        };

        var chart = new google.visualization.PieChart(document.getElementById('donutchart'));
        chart.draw(data, options);
      }
    </script>
