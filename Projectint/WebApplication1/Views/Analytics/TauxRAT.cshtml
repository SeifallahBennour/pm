    <script type="text/javascript">



        function genPDF() {
            html2canvas(document.getElementById("test"), {
                scale:2,
                onrendered: function (canvas) {
                    var img = canvas.toDataURL("image/png");
                    var doc = new jsPDF();
                    doc.addImage(img, 'JPEG',0,0);
                    doc.save('test.pdf');
                }
            });
        }
    </script>
